{
  "name": "styled-components-sorting",
  "displayName": "Styled Components Sorting",
  "description": "Styled Components Auto Sorting Extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/hoongding/styled-components-sorting"
  },
  "version": "0.0.3",
  "readme": "README.md",
  "publisher": "Hoonding",
  "engines": {
    "vscode": "^1.85.0"
  },
  "icon": "assets/styled-components-icon.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "styled-components-sorting.styled-sorting",
        "title": "Sort Styled Components",
        "category": "Styled Components Sorting"
      }
    ],
    "keybindings": [
      {
        "command": "styled-components-sorting.styled-sorting",
        "key": "cmd+shift+s",
        "mac": "cmd+shift+s",
        "win": "ctrl+shift+s",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "~18.19.7",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "eslint-plugin-airbnb": "0.0.1-security",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "3.2.2",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "chai": "^5.0.0",
    "mocha": "^10.2.0"
  }
}
